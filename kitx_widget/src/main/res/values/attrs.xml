<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="effect_selected_background" format="color|reference" />
    <attr name="effect_selected_text_color" format="color" />
    <attr name="effect_xml_selector" format="boolean" />
    <attr name="effect_round_radius" format="dimension" />
    <attr name="effect_top_left_radius" format="dimension" />
    <attr name="effect_top_right_radius" format="dimension" />
    <attr name="effect_bottom_left_radius" format="dimension" />
    <attr name="effect_bottom_right_radius" format="dimension" />
    <attr name="effect_stroke_width" format="dimension" />
    <attr name="effect_stroke_color" format="color" />
    <attr name="effect_selected_stroke_color" format="color" />
    <attr name="effect_selected_image" format="reference" />
    <attr name="effect" format="string" />
    <attr name="effects" format="string" />
    <attr name="effect_model" format="enum">
        <enum name="press" value="1" />
        <enum name="focus" value="2" />
        <enum name="both" value="3" />
        <enum name="manual" value="4" />
    </attr>

    <declare-styleable name="EffectViewContainer">
        <attr name="effect_selected_background" />
        <attr name="effect_selected_text_color" />
        <attr name="effect_xml_selector" />
        <attr name="effect_round_radius" />
        <attr name="effect_top_left_radius" />
        <attr name="effect_top_right_radius" />
        <attr name="effect_bottom_left_radius" />
        <attr name="effect_bottom_right_radius" />
        <attr name="effect_stroke_width" />
        <attr name="effect_stroke_color" />
        <attr name="effect_selected_stroke_color" />
        <attr name="effect_selected_image" />
        <attr name="effect_model" />
        <attr name="effect"/>
    </declare-styleable>

    <declare-styleable name="EffectButton">
        <attr name="effect_selected_background" />
        <attr name="effect_selected_text_color" />
        <attr name="effect_xml_selector" />
        <attr name="effect_round_radius" />
        <attr name="effect_top_left_radius" />
        <attr name="effect_top_right_radius" />
        <attr name="effect_bottom_left_radius" />
        <attr name="effect_bottom_right_radius" />
        <attr name="effect_stroke_width" />
        <attr name="effect_stroke_color" />
        <attr name="effect_selected_stroke_color" />
        <attr name="effect_selected_image" />
        <attr name="effect_model" />
        <attr name="effect"/>
    </declare-styleable>
    <declare-styleable name="EffectImageButton">
        <attr name="effect_selected_background" />
        <attr name="effect_selected_text_color" />
        <attr name="effect_xml_selector" />
        <attr name="effect_round_radius" />
        <attr name="effect_top_left_radius" />
        <attr name="effect_top_right_radius" />
        <attr name="effect_bottom_left_radius" />
        <attr name="effect_bottom_right_radius" />
        <attr name="effect_stroke_width" />
        <attr name="effect_stroke_color" />
        <attr name="effect_selected_stroke_color" />
        <attr name="effect_selected_image" />
        <attr name="effect_model" />
        <attr name="effect" />
    </declare-styleable>
    <declare-styleable name="EffectImageView">
        <attr name="effect_selected_background" />
        <attr name="effect_selected_text_color" />
        <attr name="effect_xml_selector" />
        <attr name="effect_round_radius" />
        <attr name="effect_top_left_radius" />
        <attr name="effect_top_right_radius" />
        <attr name="effect_bottom_left_radius" />
        <attr name="effect_bottom_right_radius" />
        <attr name="effect_stroke_width" />
        <attr name="effect_stroke_color" />
        <attr name="effect_selected_stroke_color" />
        <attr name="effect_selected_image" />
        <attr name="effect_model" />
        <attr name="effect"/>
    </declare-styleable>
    <declare-styleable name="EffectTextView">
        <attr name="effect_selected_background" />
        <attr name="effect_selected_text_color" />
        <attr name="effect_xml_selector" />
        <attr name="effect_round_radius" />
        <attr name="effect_top_left_radius" />
        <attr name="effect_top_right_radius" />
        <attr name="effect_bottom_left_radius" />
        <attr name="effect_bottom_right_radius" />
        <attr name="effect_stroke_width" />
        <attr name="effect_stroke_color" />
        <attr name="effect_selected_stroke_color" />
        <attr name="effect_selected_image" />
        <attr name="effect_model" />
        <attr name="effect"/>
    </declare-styleable>

    <declare-styleable name="FlowLayout">
        <attr format="dimension" name="itemSpacing"/>
        <attr format="dimension" name="lineSpacing"/>
    </declare-styleable>

    <declare-styleable name="StateConstraintLayout">
        <attr name="state_layout_placeholder_view" format="reference" />
        <attr name="state_layout_background_color" format="reference" />
        <attr name="state_layout_clickable" format="boolean"/>
    </declare-styleable>


    <declare-styleable name="TextLabel">
        <attr name="format" format="string"/>

        <attr name="startText" format="string|reference"/>
        <attr name="startTextColor" format="color|reference"/>
        <attr name="startLinkColor" format="color"/>
        <attr name="startTextSize" format="dimension"/>
        <attr name="startDrawable" format="reference"/>
        <attr name="startDrawableSize" format="dimension"/>
        <attr name="startDrawableInLast" format="boolean"/>
        <attr name="startDrawableVerticalAlignment" format="enum">
            <enum name="ALIGN_BOTTOM" value="0"/>
            <enum name="ALIGN_BASELINE" value="1"/>
            <enum name="ALIGN_CENTER" value="2"/>
        </attr>

        <attr name="endText" format="string|reference"/>
        <attr name="endTextSize" format="dimension"/>
        <attr name="endTextColor" format="color|reference"/>
        <attr name="endLinkColor" format="color"/>
        <attr name="endDrawable" format="reference"/>
        <attr name="endDrawableSize" format="dimension"/>
        <attr name="endDrawableInLast" format="boolean"/>
        <attr name="endDrawableVerticalAlignment" format="enum">
            <enum name="ALIGN_BOTTOM" value="0"/>
            <enum name="ALIGN_BASELINE" value="1"/>
            <enum name="ALIGN_CENTER" value="2"/>
        </attr>
    </declare-styleable>



    <declare-styleable name="Banner">
        <attr name="banner_interval" format="integer" />
        <attr name="banner_auto_play" format="boolean" />
        <attr name="banner_loop" format="boolean" />
        <attr name="banner_min_loop" format="integer" />
        <attr name="banner_scroll" format="boolean" />
        <attr name="banner_orientation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="IndicatorLayout">
        <attr name="indicator_width" format="dimension" />
        <attr name="indicator_height" format="dimension" />
        <attr name="indicator_margin" format="dimension" />
        <attr name="indicator_selected" format="color|reference" />
        <attr name="indicator_unselected" format="color|reference" />
        <attr name="indicator_flow" format="boolean" />
        <attr name="indicator_max_dot" format="integer" />
        <attr name="indicator_min_dot" format="integer" />
        <attr name="indicator_shape" format="enum">
            <enum name="circular" value="0" />
            <enum name="rectangle" value="1" />
        </attr>
    </declare-styleable>
</resources>
